version: "3"
services:
  zeppelin:
    build:
      context: ./docker-zeppelin-master
      dockerfile: Dockerfile
    restart: always
    ports:
     - 8888:8080
    volumes:
      - ./notebooks:/usr/zeppelin/notebook

  mlflow:
    build:
      context: ./mlflow
      dockerfile: Dockerfile
    ports:
      - 5000:5000
    volumes:
	- ./mlruns:/mlruns
