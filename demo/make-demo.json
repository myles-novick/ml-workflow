{
  "id": "/machine-learning/mldemo",
  "cmd": null,
  "cpus": 0.7,
  "mem": 1024,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "slave_public"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "splicemachine/mldemo:0.1",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "labels": {
    "HAPROXY_GROUP": "external"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "port": 5004,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "env": {
    "DEMO_PORT": "5004"
  },
  "uris": [
    "file:///etc/docker.tar.gz"
  ]
}

